pipeline
{
 agent any
 
 stages
 {
   stage('Onyueeuee')
  {
    steps{
	
     echo 'Exceuted your first step'
	 
	}
}

stage('TWooooo')
  {
    steps{
	
     input("Do you want to proceed")
	 
	}
}

stage('Threee')
  {
    steps{
	
     echo" BYeeee "
     echo" Say hi"
	 
	}
}

stage('four')
 {
  parallel{
    stage('Unit test'){
       steps{
       echo("Running unit test")
       }
      }
     stage('Integration test'){
      steps{
      echo("Running Integration test")
      }
      }
     }
    }
    }
      

}



	
